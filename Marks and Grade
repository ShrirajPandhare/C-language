#include<stdio.h>
int main()
{
	printf("Enter the Marks:");
	int Marks=100;
	scanf("%d",&Marks);
	
	if(Marks>=85)
	{
	printf("Grade A");
    }
    else if(Marks>=70 && Marks<=84)
    {
    	printf("Grade B");
	}
	else if (Marks>=55 && Marks<=69)
    {
    	printf("Grade C");
	}
	else if (Marks>40 && Marks<=54)
	{
		printf("Grade D");
		
	}
	else if (Marks<40)
	
	{
	
		printf("Grade F");
		
	}
	return 0;
	
}
